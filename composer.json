{
    "name": "nvms/php-nfa",
    "description": "Nondeterministic state machine",
    "keywords": ["nfa", "state-machine"],
    "license": "MIT",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "NVMS\\NFA\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NVMS\\NFA\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "7.2"
        },
        "sort-packages": true
    }
}
