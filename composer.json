{
    "name": "nvms/php-nfa",
    "type": "library",
    "description": "Nondeterministic state machine",
    "keywords": ["nfa", "state-machine"],
    "homepage": "http://github.com/nvms/php-nfa",
    "version": "1.0.0",
    "license": "MIT",
    "require": {
        "php": "8.2.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "phpunit/phpunit": "@stable"
    },
    "autoload": {
        "psr-4": {
            "NVMS\\NFA\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NVMS\\NFA\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "8.2.5"
        },
        "sort-packages": true
    }
}
